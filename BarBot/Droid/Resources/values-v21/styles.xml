<?xml version="1.0" encoding="UTF-8" ?>
<resources>
	<style name="BarBotTheme" parent="android:Theme.Material">
		<item name="android:statusBarColor">@color/navbar_gray</item>

		<!-- remove default ActionBar -->
		<item name="android:windowNoTitle">true</item>
    	<item name="android:windowActionBar">false</item>
		<item name="android:colorPrimary">@color/navbar_gray</item>
		<item name="android:windowBackground">@color/background_gray</item>
  	</style>

	<style name="TitleTextView" parent="android:Widget.TextView">
		<item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center</item>
		<item name="android:textSize">26sp</item>
		<item name="android:textColor">@color/text_white</item>
	</style>

	<style name="OrderButton" parent="android:Widget.Button">
    	
    </style>
</resources>